{
  "private": true,
  "name": "themix",
  "version": "0.0.1",
  "sideEffects": false,
  "engines": {
    "node": ">=16.19.1",
    "npm": ">=8.19.3"
  },
  "scripts": {
    "build": "remix build",
    "dev": "npm run build && concurrently 'npm:dev:*'",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler dev ./build/index.js --experimental-local",
    "start": "npm run build && cross-env NODE_ENV=production miniflare ./build/index.js",
    "test": "npm run build && concurrently 'npm:test:*'",
    "test:e2e": "playwright test --config=tests/e2e/playwright.e2e.ts",
    "test:unit": "playwright test --config=tests/unit/playwright.unit.ts",
    "deploy": "wrangler publish",
    "typecheck": "tsc",
    "log": "concurrently 'npm:log:*'",
    "log:deploy": "wrangler tail",
    "clean": "rimraf .cache public/build",
    "clean:modules": "rimraf node_modules",
    "OLD_test": "playwright test",
    "OLD_dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "OLD_start": "npm run build && cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.14.3",
    "@remix-run/cloudflare-workers": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3",
    "@miniflare/tre": "^3.0.0-next.12",
    "@playwright/test": "^1.32.3",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "miniflare": "^2.13.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.3.2",
    "tailwindcss": "^3.3.0",
    "tslib": "^2.5.0",
    "turbo": "^1.8.5",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "wrangler": "^2.13.0"
  }
}
