{
  "private": true,
  "name": "remix",
  "version": "0.0.1",
  "sideEffects": false,
  "engines": {
    "node": ">=16.19.1",
    "npm": ">=8.19.3"
  },
  "scripts": {
    "build": "remix build",
    "dev": "concurrently 'npm:dev:*'",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "start": "npm run build && cross-env NODE_ENV=production miniflare ./build/index.js",
    "test": "playwright test",
    "deploy": "wrangler publish",
    "typecheck": "tsc",
    "log": "concurrently 'npm:log:*'",
    "log:deploy": "wrangler tail"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.14.3",
    "@remix-run/cloudflare-workers": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "isbot": "latest",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230321.0",
    "@playwright/test": "^1.32.1",
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "miniflare": "^2.13.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "turbo": "^1.8.5",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "wrangler": "^2.13.0"
  }
}
